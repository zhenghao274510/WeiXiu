<view class="container"><view class="tui-banner-box"><swiper class="tui-banner-swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{150}}" circular="{{true}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="tui-banner-item"><image class="{{['tui-slide-image',current!=index?'tui-banner-scale':'']}}" src="{{item.image}}" mode="scaleToFill" data-event-opts="{{[['tap',[['detail',['$0'],[[['banner','',index]]]]]]]}}" bindtap="__e"></image></swiper-item></block></swiper></view><view class="tui-rolling-news list-item"><tui-icon vue-id="1" name="news" size="{{22}}" color="#555" bind:__l="__l"></tui-icon><swiper class="tui-swiper" vertical="{{true}}" autoplay="{{true}}" circular="{{true}}" interval="{{4000}}"><block wx:for="{{headlines}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="tui-swiper-item"><view class="tui-news-item">{{item}}</view></swiper-item></block></swiper></view><scroll-view scroll-x="true"><view class="tui-classify-box"><view class="tui-classify-box-one"><block wx:for="{{classify}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<=9}}"><view class="tui-classify-item" data-key="{{item.name}}" data-event-opts="{{[['tap',[['more',['$event']]]]]}}" bindtap="__e"><image class="tui-classify-img" src="{{'/static/images/classify/'+item.img+'.png'}}"></image><view class="tui-classify-name">{{item.name}}</view></view></block></block></view><block wx:if="{{classify.length>10}}"><view class="tui-classify-box-one"><block wx:for="{{classify}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index>9}}"><view class="tui-classify-item" data-key="{{item.name}}" data-event-opts="{{[['tap',[['more',['$event']]]]]}}" bindtap="__e"><image class="tui-classify-img" src="{{'/static/images/classify/'+item.img+'.png'}}"></image><view class="tui-classify-name">{{item.name}}</view></view></block></block></view></block></view></scroll-view><view class="nav_list"><block wx:for="{{keywordList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['detail',['$0'],[[['keywordList','',k]]]]]]]}}" class="nav_list_item" bindtap="__e"><block wx:if="{{k==0}}"><image class="keyWordImg" src="/static/img/急速退款@2x.png" mode></image></block><block wx:if="{{k==1}}"><image class="keyWordImg" src="/static/img/专业团队@2x.png" mode></image></block><block wx:if="{{k==2}}"><image class="keyWordImg" src="/static/img/保障@2x.png" mode></image></block><block wx:if="{{k==3}}"><image class="keyWordImg" src="/static/img/service-全国联保@2x.png" mode></image></block><text class="{{['content',(k==keywordList.length-1)?'noBorder':'']}}">{{v.title}}</text></view></block></view><view class="footer-list"><block wx:for="{{footerList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['pinPaiList',['$0'],[[['footerList','',index]]]]]]]}}" class="{{['footer-list-item',[(index==0||index==3)?'footer_list_one':''],[(index==1||index==2)?'footer_list_two':''],[(index==4)?'end':''],[(index%2==1)?'marginR':'']]}}" bindtap="__e"><image class="footer-list-item-image" src="{{item.image}}" mode="scaleToFill"></image></view></block></view><view data-event-opts="{{[['tap',[['gotoFinished',['$event']]]]]}}" class="footer-icon" bindtap="__e"><image src="/static/img/我要预约@2x.png" mode="scaleToFill"></image></view></view>