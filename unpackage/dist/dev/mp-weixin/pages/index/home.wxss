  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  background: #fff;
}
.tui-rolling-news::after {
  left: 0;
}
.tui-swiper {
  margin-left: 8rpx;
  font-size: 28rpx;
  height: 80rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.tui-swiper-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tui-news-item {
  line-height: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #555;
}
